<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/frameLayout"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="#030D2E"
    tools:context=".DrivingAndControlFragment">

    <TextView
        android:id="@+id/textView10"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="107dp"
        android:layout_marginTop="40dp"
        android:layout_marginEnd="107dp"
        android:text="DRIVING AND CONTROL WITH CAMERA"
        android:textColor="#FFFFFF"
        android:textSize="18sp"
        android:textStyle="normal|bold"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintLeft_toLeftOf="parent"
        app:layout_constraintRight_toRightOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

    <Button
        android:id="@+id/forward"
        android:layout_width="110dp"
        android:layout_height="50dp"
        android:layout_marginStart="150dp"
        android:layout_marginTop="28dp"
        android:layout_marginEnd="166dp"
        android:layout_marginBottom="71dp"
        android:text="FORWARD"
        app:layout_constraintBottom_toTopOf="@+id/back"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/videoView" />

    <Button
        android:id="@+id/right"
        android:layout_width="110dp"
        android:layout_height="50dp"
        android:layout_marginStart="114dp"
        android:layout_marginBottom="12dp"
        android:text="RIGHT"
        app:layout_constraintBottom_toTopOf="@+id/back"
        app:layout_constraintStart_toEndOf="@+id/left"
        app:layout_constraintTop_toBottomOf="@+id/videoView" />

    <Button
        android:id="@+id/back"
        android:layout_width="110dp"
        android:layout_height="50dp"
        android:layout_marginBottom="38dp"
        android:text="BACK"
        app:layout_constraintBottom_toTopOf="@+id/lightsoff"
        app:layout_constraintEnd_toStartOf="@+id/right"
        app:layout_constraintStart_toEndOf="@+id/lightson"
        app:layout_constraintTop_toBottomOf="@+id/right" />

    <Button
        android:id="@+id/left"
        android:layout_width="110dp"
        android:layout_height="50dp"
        android:layout_marginStart="36dp"
        android:layout_marginTop="84dp"
        android:layout_marginEnd="114dp"
        android:layout_marginBottom="103dp"
        android:text="LEFT"
        app:layout_constraintBottom_toTopOf="@+id/lightson"
        app:layout_constraintEnd_toStartOf="@+id/right"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/videoView" />

    <VideoView
        android:id="@+id/videoView"
        android:layout_width="0dp"
        android:layout_height="0dp"
        android:layout_marginStart="25dp"
        android:layout_marginTop="87dp"
        android:layout_marginEnd="25dp"
        android:layout_marginBottom="87dp"
        app:layout_constraintBottom_toTopOf="@+id/right"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

    <Button
        android:id="@+id/lightson"
        android:layout_width="96dp"
        android:layout_height="64dp"
        android:layout_marginStart="54dp"
        android:layout_marginEnd="110dp"
        app:layout_constraintBottom_toBottomOf="@+id/lightsoff"
        app:layout_constraintEnd_toStartOf="@+id/lightsoff"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="@+id/lightsoff"
        tools:text="LIGHTS ON" />

    <Button
        android:id="@+id/lightsoff"
        android:layout_width="96dp"
        android:layout_height="64dp"
        android:layout_marginEnd="70dp"
        android:layout_marginBottom="26dp"
        app:layout_constraintBottom_toTopOf="@+id/textView"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toEndOf="@+id/lightson"
        app:layout_constraintTop_toBottomOf="@+id/back"
        tools:text="LIGHTS OFF" />

    <SeekBar
        android:id="@+id/seekBar"
        android:layout_width="0dp"
        android:layout_height="33dp"
        android:layout_marginTop="20dp"
        android:layout_marginEnd="43dp"
        android:background="@color/yellow"
        android:backgroundTint="@color/white"
        android:visibility="visible"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toEndOf="@+id/textView"
        app:layout_constraintTop_toBottomOf="@+id/lightson" />

    <TextView
        android:id="@+id/textView"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="25dp"
        android:layout_marginEnd="23dp"
        android:layout_marginBottom="49dp"
        android:text="SPEED"
        android:textColor="@color/white"
        android:textColorLink="@color/white"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toStartOf="@+id/seekBar"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/lightsoff" />

</androidx.constraintlayout.widget.ConstraintLayout>