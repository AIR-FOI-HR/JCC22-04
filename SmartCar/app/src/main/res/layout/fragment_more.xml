<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/frameLayout3"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="#030D2E"
    tools:context=".MoreFragment">

    <TextView
        android:id="@+id/textView9"
        android:layout_width="147dp"
        android:layout_height="wrap_content"
        android:text="@string/app_name"
        android:textColor="#FFD247"
        android:textSize="34sp"
        android:textStyle="bold"
        tools:layout_editor_absoluteX="132dp"
        tools:layout_editor_absoluteY="100dp" />

    <TextView
        android:id="@+id/textView10"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="PROFILE INFO"
        android:textColor="#FFFFFF"
        android:textSize="18sp"
        android:textStyle="normal|bold"
        tools:layout_editor_absoluteX="149dp"
        tools:layout_editor_absoluteY="184dp" />

    <TextView
        android:id="@+id/textView5"
        android:layout_width="187dp"
        android:layout_height="wrap_content"
        android:text="Remote control of your"
        android:textColor="#FFFFFF"
        android:textSize="18sp"
        android:textStyle="normal"
        tools:layout_editor_absoluteX="112dp"
        tools:layout_editor_absoluteY="76dp" />

    <ImageView
        android:id="@+id/imageView2"
        android:layout_width="237dp"
        android:layout_height="154dp"
        android:src="@drawable/smartcar3"
        tools:layout_editor_absoluteX="87dp"
        tools:layout_editor_absoluteY="208dp" />

    <TextView
        android:id="@+id/textViewLogOut"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="@string/log_out"
        android:textColor="#FFFFFF"
        android:textSize="18sp"
        tools:layout_editor_absoluteX="178dp"
        tools:layout_editor_absoluteY="626dp" />

    <TextView
        android:id="@+id/textViewEdit"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="@string/edit"
        android:textSize="18sp"
        tools:layout_editor_absoluteX="306dp"
        tools:layout_editor_absoluteY="378dp" />

    <EditText
        android:id="@+id/editEmailAdressProfileInfo"
        android:layout_width="300dp"
        android:layout_height="50dp"
        android:background="#121D3E"
        android:backgroundTint="#121D3E"
        android:ems="10"
        android:hint="   Email"
        android:inputType="textEmailAddress"
        android:textColor="#FFFFFF"
        android:textColorHint="#80FFFFFF"
        tools:layout_editor_absoluteX="56dp"
        tools:layout_editor_absoluteY="489dp" />

    <EditText
        android:id="@+id/editPasswordProfileInfo"
        android:layout_width="300dp"
        android:layout_height="50dp"
        android:background="#121D3E"
        android:backgroundTint="#121D3E"
        android:ems="10"
        android:hint="   Password"
        android:inputType="textPassword"
        android:textColor="#FFFFFF"
        android:textColorHint="#80FFFFFF"
        tools:layout_editor_absoluteX="56dp"
        tools:layout_editor_absoluteY="562dp" />

    <EditText
        android:id="@+id/editTextName"
        android:layout_width="300dp"
        android:layout_height="50dp"
        android:background="#121D3E"
        android:backgroundTint="#121D3E"
        android:ems="10"
        android:hint="  Name"
        android:inputType="textEmailAddress"
        android:textColor="#FFFFFF"
        android:textColorHint="#80FFFFFF"
        tools:layout_editor_absoluteX="56dp"
        tools:layout_editor_absoluteY="417dp" />

</androidx.constraintlayout.widget.ConstraintLayout>